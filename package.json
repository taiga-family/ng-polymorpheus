{
    "name": "ng-polymorpheus",
    "version": "4.9.0",
    "private": true,
    "description": "This is a small library for customizing components appearance",
    "keywords": [
        "angular",
        "ng",
        "template",
        "polymorph",
        "component",
        "function",
        "context",
        "ng-template",
        "TemplateRef",
        "ngTemplateOutlet",
        "ngComponentOutlet"
    ],
    "homepage": "https://github.com/taiga-family/ng-polymorpheus#README",
    "bugs": "https://github.com/taiga-family/ng-polymorpheus/issues",
    "repository": "https://github.com/taiga-family/ng-polymorpheus",
    "license": "Apache-2.0",
    "author": {
        "name": "Alex Inkin",
        "email": "alexander@inkin.ru"
    },
    "contributors": [
        "Roman Sedov <79601794011@ya.ru> (http://marsibarsi.me/)"
    ],
    "workspaces": [
        "projects/*"
    ],
    "scripts": {
        "prepare": "husky",
        "start": "nx serve demo",
        "dev:ssr": "nx run demo:serve-ssr",
        "build:ssr": "nx build --prod && nx run demo:server:production",
        "prerender": "nx run demo:prerender",
        "build": "nx build demo",
        "stylelint": "stylelint '**/*.{less,css}'",
        "prettier": "prettier !package-lock.json !CHANGELOG.md .  --ignore-path .gitignore",
        "test": "nx test ng-polymorpheus",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit --skipLibCheck",
        "release": "npx syncer && npx nx publish ng-polymorpheus"
    },
    "commitlint": {
        "extends": [
            "@taiga-ui/commitlint-config"
        ]
    },
    "lint-staged": {
        "*.less": [
            "stylelint --fix"
        ],
        "*.{js,ts,html}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{svg,yml,md,less,json}": [
            "prettier --write"
        ]
    },
    "browserslist": [
        "extends @taiga-ui/browserslist-config"
    ],
    "prettier": "@taiga-ui/prettier-config",
    "eslintConfig": {
        "extends": [
            "plugin:@taiga-ui/experience/all"
        ],
        "rules": {
            "@angular-eslint/no-conflicting-lifecycle": "off"
        },
        "root": true
    },
    "stylelint": {
        "extends": [
            "@taiga-ui/stylelint-config"
        ]
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "16.2.16",
        "@angular-devkit/core": "16.2.16",
        "@angular/animations": "16.2.12",
        "@angular/cli": "16.2.16",
        "@angular/common": "16.2.12",
        "@angular/compiler": "16.2.12",
        "@angular/compiler-cli": "16.2.12",
        "@angular/core": "16.2.12",
        "@angular/forms": "16.2.12",
        "@angular/language-service": "16.2.12",
        "@angular/platform-browser": "16.2.12",
        "@angular/platform-browser-dynamic": "16.2.12",
        "@angular/platform-server": "16.2.12",
        "@angular/router": "16.2.12",
        "@ng-web-apis/common": "4.12.0",
        "@ng-web-apis/universal": "4.12.0",
        "@nx/angular": "20.1.4",
        "@nx/jest": "20.1.4",
        "@nx/node": "20.1.4",
        "@nx/workspace": "20.1.4",
        "@swc/helpers": "0.5.17",
        "@taiga-ui/auto-changelog-config": "0.277.0",
        "@taiga-ui/browserslist-config": "0.277.0",
        "@taiga-ui/commitlint-config": "0.277.0",
        "@taiga-ui/dompurify": "4.1.11",
        "@taiga-ui/eslint-plugin-experience": "0.249.0",
        "@taiga-ui/prettier-config": "0.277.0",
        "@taiga-ui/release-it-config": "0.277.0",
        "@taiga-ui/stylelint-config": "0.277.0",
        "@taiga-ui/syncer": "0.277.0",
        "@taiga-ui/tsconfig": "0.277.0",
        "@types/estree": "1.0.7",
        "@types/express": "5.0.1",
        "@types/jest": "29.5.14",
        "@types/node": "22.15.13",
        "express": "4.21.2",
        "highlight.js": "11.11.1",
        "husky": "9.1.7",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "jest-preset-angular": "14.5.5",
        "lint-staged": "15.5.2",
        "ng-packagr": "16.2.3",
        "ngx-highlightjs": "10.0.0",
        "nx": "20.1.4",
        "rxjs": "7.8.2",
        "ts-jest": "29.3.2",
        "ts-node": "10.9.2",
        "typescript": "5.0.4",
        "webpack": "5.99.8"
    },
    "syncer": {
        "includePaths": [
            "./projects",
            "./package-lock.json"
        ],
        "matchPackageNames": [
            "@taiga-ui/polymorpheus",
            "ng-polymorpheus"
        ],
        "ignorePackageNames": []
    }
}
